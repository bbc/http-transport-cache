{
  "name": "@bbc/http-transport-cache",
  "version": "5.0.0",
  "description": "Caching middleware",
  "main": "index.js",
  "scripts": {
    "test": "mocha && pnpm run lint",
    "coverage": "nyc mocha && nyc report --reporter=html && nyc report --reporter=json-summary",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bbc/http-transport-cache.git"
  },
  "bugs": {
    "url": "https://github.com/bbc/http-transport-cache/issues"
  },
  "engines": {
    "node": ">=8.13.0",
    "pnpm": ">=10"
  },
  "keywords": [
    "http-transport",
    "caching",
    "http",
    "client",
    "middleware"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@bbc/http-transport": "^4.3.0",
    "@bbc/http-transport-to-error": "^2.2.0",
    "chai": "^4.1.2",
    "eslint": "^8.54.0",
    "eslint-config-iplayer": "^9.1.0",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.3.0",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3"
  },
  "dependencies": {
    "@hapi/catbox": "^12.1.1",
    "@hapi/catbox-memory": "^6.0.1",
    "@hapi/wreck": "^18.0.1",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.20"
  },
  "peerDependencies": {
    "@bbc/http-transport": "^3 || ^4"
  },
  "eslintConfig": {
    "extends": "iplayer",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
      "overrides": [
    {
      "files": ["test/**/*.js"], 
      "rules": {
        "listeners/no-inline-function-event-listener": "off",
        "listeners/no-missing-remove-event-listener": "off"
      }
    }
  ]
  },
  "mocha": {
    "recursive": true,
    "exit": true
  }
}